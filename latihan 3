kelas = input('masukkan kelas')
alamat = input('masukkan alamat')
print(' ') 
print('Output Data Siswa') 
print('nisn siswa adalah', nisn) 
print('nama siswa adalah', nama) 
print('kelas siswa adalah', kelas)
print('alamat siswa adalah', alamat)
